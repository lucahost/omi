{
    "container_image": "centos:7",
    "package_manager": "yum",
    "microsoft_repo": "https://packages.microsoft.com/config/rhel/7/prod.repo",
    "openssl_version": "3.0.0-alpha13",
    "build_deps": [
        "ca-certificates",
        "cmake",
        "gcc",
        "git",
        "krb5-devel",
        "make",
        "perl-Data-Dumper",
        "perl-IPC-Cmd",
        "wget",
        "which",
        "zlib-devel"
    ],
    "test_deps": [
        "ca-certificates",
        "gssntlmssp",
        "krb5-devel",
        "krb5-workstation",
        "powershell",
        "which"
    ],
    "cert_staging_dir": "/etc/pki/ca-trust/source/anchors",
    "cert_staging_cmd": "update-ca-trust extract"
}
